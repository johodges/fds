&HEAD CHID='McCaffrey_45kW_12mm'/

&MULT ID='MESH-MULT', DX=0.3, DY=0.3, DZ=0.2875, I_UPPER=3, J_UPPER=3, K_UPPER=7/
&MESH IJK=24,24,23, XB=-0.6,-0.3,-0.6,-0.3,-0.75,-0.4625, MULT_ID='MESH-MULT'/ 128 mesh

&TIME T_END=300./
&DUMP DT_BNDF=1, DT_DEVC=0.1, WRITE_XYZ=T, RESULTS_DIR='McCaffrey_45kW_12mm'/

&RADI NUMBER_RADIATION_ANGLES=400, TIME_STEP_INCREMENT=12, C_MIN=0.1, C_MAX=1000, QR_CLIP=-1/
&MISC SIMULATION_MODE='LES'/

&MATL ID= 'CONCRETE', FYI='Drysdale, Intro to Fire Dynamics', SPECIFIC_HEAT=0.90, DENSITY=2000., CONDUCTIVITY=1.0/
&SURF ID = 'FLOOR', MATL_ID='CONCRETE', THICKNESS=0.1, COLOR='GRAY'/

&REAC FUEL='METHANE', SOOT_YIELD=0.00082, CO_YIELD=0.00035, N_SIMPLE_CHEMISTRY_REACTIONS=2, / 
&SURF ID='fire', HRRPUA=500., TMP_FRONT=100, COLOR='RED'/
&OBST XB=-0.15,0.15,-0.15,0.15,-0.75,0.0, SURF_IDS='fire','INERT','INERT'/

! ignitor
&INIT XB=-0.15,0.15,-0.15,0.15,0.0,0.1, HRRPUV=1000, RAMP_Q='ignition-ramp'/
&RAMP ID='ignition-ramp', T=0, F=1/
&RAMP ID='ignition-ramp', T=20,F=1/
&RAMP ID='ignition-ramp', T=30,F=0/

&VENT DB='XMIN', SURF_ID='OPEN'/
&VENT DB='XMAX', SURF_ID='OPEN'/
&VENT DB='YMIN', SURF_ID='OPEN'/
&VENT DB='YMAX', SURF_ID='OPEN'/
&VENT DB='ZMIN', SURF_ID='FLOOR'/
&VENT DB='ZMAX', SURF_ID='OPEN'/

&DEVC ID='RTE_C', XYZ=0.0,0.0,0.5, QUANTITY='RTE SOURCE CORRECTION FACTOR' /

&DEVC XB=0,0,0,0,0.025,1.375, POINTS=55, QUANTITY='HRRPUV',         ID='hrrpul',  DX=0.6, DY=0.6, DZ=0.05, SPATIAL_STATISTIC='VOLUME INTEGRAL', CONVERSION_FACTOR=0.050 /
&DEVC XB=0,0,0,0,0.025,1.375, POINTS=55, QUANTITY='RADIATION LOSS', ID='qradpul', DX=0.6, DY=0.6, DZ=0.05, SPATIAL_STATISTIC='VOLUME INTEGRAL', CONVERSION_FACTOR=0.050, HIDE_COORDINATES=T /
&DEVC XB=0,0,0,0,0.025,1.375, POINTS=55, QUANTITY='W-VELOCITY', ID='velocity', /
&DEVC XB=0,0,0,0,0.025,1.375, POINTS=55, QUANTITY='TEMPERATURE', ID='temperature', HIDE_COORDINATES=T /

