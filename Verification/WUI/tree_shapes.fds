&DEVC ID='RANDOM CYLINDRICAL SHELL',  XB=2.5,2.9,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/

&DEVC ID='1PPC BOX', XB=3.1,3.5,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/
&DEVC ID='1PPC CONE',  XB=3.6,4.2,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/
&DEVC ID='1PPC CYLINDER',  XB=4.3,4.7,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/
&DEVC ID='1PPC CONICAL SHELL',  XB=4.9,5.4,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/
&DEVC ID='1PPC CYLINDRICAL SHELL',  XB=5.5,5.9,0.2,0.8,0.2,0.8, PART_ID='needles', QUANTITY='SURFACE DENSITY', SPATIAL_STATISTIC='SURFACE INTEGRAL'/

&TAIL /
